<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.prosystem"
		name="Atari - 7800 (ProSystem)"
		version="1.3.0.44"
		provider-name="Greg Stanton, Brian Berlin, Leonis, Greg DeMent">
	<requires>
