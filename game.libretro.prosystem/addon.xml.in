<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.prosystem"
		name="Atari - 7800 (ProSystem)"
		version="1.3.0.30"
		provider-name="Greg Stanton, Brian Berlin, Leonis, Greg DeMent">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.atari.7800.proline" version="1.0.0"/>
		<import addon="game.controller.atari.7800.gamepad" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>7800</platforms>
		<extensions>a78|bin|cdf</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>false</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.prosystem</source>
		<assets>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
		<summary lang="en_GB">Atari - 7800 (ProSystem) 1.3e</summary>
		<description lang="da_DK">ProSystem er en Atari 7800-emulator.[CR][CR]Placer [I]7800 BIOS (U).rom[/I] (valgfrit) i emulatorens brugerdatamappe.</description>
		<description lang="en_GB">ProSystem is an Atari 7800 emulator.[CR][CR]Place [I]7800 BIOS (U).rom[/I] (optional) in the emulator&apos;s userdata folder.</description>
		<description lang="es_ES">ProSystem es un emulador de Atari 7800.[CR][CR]Coloca [I]7800 BIOS (U).rom[/I] (opcional) en la carpeta userdata del emulador.</description>
		<description lang="zh_CN">ProSystem 是 Atari 7800 模拟器。[CR][CR]将 [I]7800 BIOS (U).rom[/I]（可选）放到模拟器的 userdata 文件夹中。</description>
	</extension>
</addon>
